{
  "name": "@cotype/build-client",
  "version": "1.0.6",
  "description": "",
  "main": "lib/index.js",
  "bin": {
    "cotype-build-client": "./lib/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "generate-demo": "npm run prepare && rm -f demo/Api.ts && ./lib/cli.js ./demo/swagger.json ./demo/Api.ts",
    "prepare": "npm run build && chmod +x ./lib/cli.js",
    "test": "npm run generate-demo && jest"
  },
  "author": "Felix Gnass",
  "license": "MIT",
  "dependencies": {
    "@loopback/openapi-v3-types": "^1.0.21",
    "find-up": "^4.0.0",
    "oazapfts": "^1.2.0",
    "pascal-case": "^2.0.1",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/find-up": "^2.1.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts",
      "d.ts"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    }
  }
}
